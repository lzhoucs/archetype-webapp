<archetype-descriptor xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
                      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                      xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd">
    <id>webapp</id>
    <allowPartial>false</allowPartial>
    <sources>
        <!-- The encoding to be used when reading/writing this file. Platform encoding is used by default, or ISO-8859-1 when filename ends in .properties -->
        <source encoding="UTF-8">src/main/java/Dummy.java</source>
    </sources>
    <resources>
        <resource encoding="UTF-8">src/resources/logback.xml</resource>
    </resources>

    <testSources>
        <source encoding="UTF-8">src/test/java/Dummy.java</source>
    </testSources>
    <testResources>
        <resource encoding="UTF-8">src/resources/logback.xml</resource>
    </testResources>
</archetype-descriptor>